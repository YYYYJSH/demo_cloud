            <artifactId>spring-cloud-alibaba-nacos-config</artifactId>
        </dependency>
        <dependency>
            <groupId>clould_demo</groupId>
            <artifactId>pub_demo</artifactId>
            <version>1.0-SNAPSHOT</version>
        </dependency>
    </dependencies>

